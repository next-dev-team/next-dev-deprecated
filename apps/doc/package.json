{
  "private": true,
  "name": "@next-dev/docs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "dumi dev",
    "build": "dumi build && pnpm build-tailwind",
    "publish": "gh-pages -d ./dist",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "build-tailwind": "npx tailwindcss -o global.css --minify"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "dumi": "^1.1.42",
    "lint-staged": "^10.0.7",
    "yorkie": "^2.0.0",
    "tailwindcss": "^3.1.4",
    "postcss": "^8.4.14",
    "autoprefixer": "^10.4.7",
    "deploy:gh": "gh-pages -d ./dist",
    "gh-pages": "^4.0.0",
    "ahooks": "^3.5.0",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.3",
    "@umijs/preset-react": "^2.1.4",
    "@types/node": "^17.0.12",
    "@types/react": "17.0.37",
    "@umijs/ssr-darkreader": "^4.9.45",
    "antd": "^4.21.3",
    "dumi-theme-default": "^1.1.21",
    "react-lazyload": "^3.2.0",
    "@ant-design/pro-skeleton": "^1.0.9",
    "@ant-design/pro-components": "^1.1.6"
  },
  "dependencies": {
    "ui": "workspace:*",
    "utils": "workspace:*"
  }
}
