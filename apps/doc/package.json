{
  "private": true,
  "name": "@next-dev/docs",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "dev": "dumi dev",
    "build": "pnpm build-tailwind && dumi build",
    "build:only": "dumi build",
    "publish": "gh-pages -d ./dist",
    "prettier": "prettier --write \"**/*.{js,jsx,tsx,ts,less,md,json}\"",
    "build-tailwind": "npx tailwindcss -o tailwind-prod.css --minify"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,less,md,json}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "devDependencies": {
    "@ant-design/icons": "^4.7.0",
    "@ant-design/pro-components": "^1.1.6",
    "@ant-design/pro-skeleton": "^1.0.9",
    "@types/node": "^17.0.12",
    "@types/react": "17.0.37",
    "@types/react-syntax-highlighter": "^15.5.3",
    "@umijs/preset-react": "^2.1.4",
    "@umijs/ssr-darkreader": "^4.9.45",
    "ahooks": "^3.5.0",
    "antd": "^4.21.4",
    "autoprefixer": "^10.4.7",
    "babel-plugin-import": "^1.13.5",
    "deploy:gh": "gh-pages -d ./dist",
    "dumi": "^1.1.45",
    "dumi-theme-default": "^1.1.21",
    "gh-pages": "^4.0.0",
    "lint-staged": "^10.0.7",
    "postcss": "^8.4.14",
    "react-element-to-jsx-string": "^15.0.0",
    "react-lazyload": "^3.2.0",
    "react-syntax-highlighter": "^15.5.0",
    "tailwindcss": "^3.1.6",
    "tsconfig": "workspace:*",
    "typescript": "^4.5.3",
    "yorkie": "^2.0.0"
  },
  "dependencies": {
    "antd-ui": "workspace:*",
    "ui": "workspace:*",
    "utils": "workspace:*"
  }
}
