{
  "name": "next-dev",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "doc:dev": "turbo run dev",
    "doc:build": "turbo run build --filter ./apps/doc...",
    "doc:predeploy": "turbo run publish --filter ./apps/doc...",
    "doc:deploy": "pnpm doc:build && pnpm doc:predeploy",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm"
  },
  "devDependencies": {
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "pnpm@7.3.0"
}
