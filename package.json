{
  "name": "next-dev",
  "author": "Next-Dev",
  "private": true,
  "packageManager": "pnpm@7.3.0",
  "description": "ðŸš€ All in one fullstack Development solutions",
  "main": ".eslintrc.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/next-dev-team/next-dev.git"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/next-dev-team/next-dev/issues"
  },
  "homepage": "https://github.com/next-dev-team/next-dev#readme",
  "workspaces": [
    "examples/*",
    "boilerplate/*",
    "docs/*",
    "packages/*"
  ],
  "scripts": {
    "dev:dumi": "dumi dev",
    "build:dumi": "dumi build",
    "prepare": "dumi setup",
    "setup": "pnpm install",
    "build": "turbo run build",
    "dev": "concurrently \"turbo run dev --parallel\" \"dumi dev\"",
    "doc:dev": "dumi dev",
    "doc:build": "pnpm clean &&  turbo run build --filter ./doc... && pnpm build:dumi",
    "doc:predeploy": "turbo run publish --filter ./boilerplate/doc...",
    "doc:deploy": "pnpm doc:build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md,json}\"  --ignore-path .gitignore",
    "preinstall": "npx only-allow pnpm",
    "clean": "turbo run clean",
    "publish": "gh-pages -d ./dist",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release:packages": "turbo run build --filter=packages^... && changeset publish"
  },
  "devDependencies": {
    "@ant-design/icons": "^4.8.0",
    "@changesets/cli": "^2.25.2",
    "@emotion/babel-preset-css-prop": "^11.10.0",
    "@emotion/react": "^11.10.5",
    "@next-dev/utils": "workspace:1.0.11-rc4",
    "@types/node": "^18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "^18.0.9",
    "antd": "^5.0.1",
    "antd-token-previewer": "^1.1.0-13",
    "babel-plugin-import": "^1.13.5",
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "dumi": "^2.0.6",
    "eslint": "^7.32.0",
    "eslint-ast-utils": "^1.1.0",
    "eslint-config-custom": "workspace:0.0.0",
    "eslint-config-next": "^12.3.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-formatter-pretty": "^4.1.0",
    "eslint-import-resolver-node": "^0.3.6",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-module-utils": "^2.7.4",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^24.7.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.31.11",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-unicorn": "^20.1.0",
    "eslint-rule-composer": "^0.3.0",
    "eslint-rule-docs": "^1.1.235",
    "eslint-scope": "^5.1.1",
    "eslint-template-visitor": "^2.3.2",
    "eslint-utils": "^2.1.0",
    "eslint-visitor-keys": "^2.1.0",
    "gh-pages": "^4.0.0",
    "prettier": "latest",
    "prettier-plugin-organize-imports": "^2.3.4",
    "prettier-plugin-packagejson": "^2.3.0",
    "prettier-plugin-two-style-order": "^1.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "stylelint-config-prettier": "^8.0.2",
    "tailwind-merge": "^1.8.0",
    "tsup": "^6.5.0",
    "turbo": "latest",
    "typescript": "^4.9.3",
    "utility-types": "^3.10.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
