{
  "name": "next-dev",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "setup": "pnpm install",
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "doc:dev": "turbo run dev --filter ./apps/doc...",
    "doc:build": "turbo run build --filter ./apps/doc...",
    "doc:predeploy": "turbo run publish --filter ./apps/doc...",
    "doc:deploy": "pnpm doc:build",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "preinstall": "npx only-allow pnpm",
    "clean": "turbo run clean && rm -rf node_modules",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "turbo run build --filter=docs^... && changeset publish"
  },
  "devDependencies": {
    "@changesets/cli": "^2.25.0",
    "@types/node": "^18.0.0",
    "@types/react-dom": "^17.0.11",
    "babel-plugin-import": "^1.13.5",
    "concurrently": "^7.2.2",
    "eslint-config-custom": "workspace:*",
    "prettier": "latest",
    "react-dom": "^17.0.0",
    "tailwind-merge": "^1.6.1",
    "tsup": "^6.2.3",
    "turbo": "latest",
    "typescript": "^4.8.4"
  },
  "engines": {
    "npm": ">=7.0.0",
    "node": ">=14.0.0"
  },
  "packageManager": "pnpm@7.3.0"
}