{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "skipLibCheck": true,
    "jsx": "react-jsxdev",
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "strict": false,
    "paths": {
      "@/*": ["./*"],
      "@@/*": ["./docs/.umi/*"]
    },
    "allowSyntheticDefaultImports": true
  },
  "exclude": [
    "node_modules",
    "node_modules/*",
    "./node_modules",
    "./node_modules/*",
    "lib",
    "es",
    "dist",
    "**/__test__",
    "test",
    "tests"
  ]
}
